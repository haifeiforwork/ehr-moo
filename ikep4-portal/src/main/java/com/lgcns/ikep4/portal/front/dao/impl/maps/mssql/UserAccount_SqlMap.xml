<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

<sqlMap namespace="portal.front.dao.UserAccount">
	
	<select id="getAllUSerCount" resultClass="String">
      <![CDATA[
		SELECT 
           	COUNT(*)
		FROM 
			IKEP4_EV_USER 
        WHERE USER_STATUS='C'
      ]]>
	</select>
	
</sqlMap>